{
  "name": "quotes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "vue": "vue-cli-service build --mode development --watch",
    "lint": "vue-cli-service lint",
    "server": "nodemon server/app.js",
    "start": "npm run build && node server/app.js"
  },
  "dependencies": {
    "EventEmitter": "^1.0.0",
    "auth0-js": "^9.8.2",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "connect-history-api-fallback": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "mongoose": "^5.3.13",
    "morgan": "^1.9.1",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "babel-plugin-root-import": "^6.1.0",
    "nodemon": "^1.18.6",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "rules": {
      "vue/require-prop-types": false,
      "no-var": [
        "warn"
      ],
      "prefer-const": [
        "warn"
      ],
      "no-console": "off",
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
